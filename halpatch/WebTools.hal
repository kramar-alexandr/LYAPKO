remote function Time GetCurTimeOnServ();external procedure ExtractObjWithSeparator(string,string,Boolean,var Integer,var string);globalprocedure MyBIRun()begin  string 255 url;  integer compnr;  record ProgramStatusBlock PSb;	record CompaniesBlock Cb;	row CompaniesBlock Cbrw;  record RcVc RepSpec;	integer wn,pos;  string 20 sd,ed;  wn = curwindow;	deselectwindow(wn,true);	getwindowrecord(wn,RepSpec);	blockload(PSb);	blockload(Cb);    ExtractObjWithSeparator(":",RepSpec.Period2Str,true,pos,sd);  ExtractObjWithSeparator(":",RepSpec.Period2Str,true,pos,ed);	compnr = currentcompany;	matrowget(Cb,compnr-1,Cbrw);  url = "http://" & Cbrw.TCPIP & ":" & PSb.httpPort & "/SaleStat.html?param=" & MD5String(GetHour(GetCurTimeOnServ) & ":" & GetMinute(GetCurTimeOnServ));	//url = "http://" & Cbrw.TCPIP & ":" & PSb.httpPort & "/SaleStat.html?param=" & MD5String(GetHour(GetCurTimeOnServ) & ":" & GetMinute(GetCurTimeOnServ)) & "&sd=" & sd & "&ed=" & ed & "&loc=" & RepSpec.f1;  OpenWebBrowser(url);	return;end;